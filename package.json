        "@tsparticles/react": "^3.0.0",
        "@tsparticles/slim": "3.9.1",
        "@types/node": "20.19.32",
        "@types/react": "19.2.13",
        "@types/react-dom": "19.2.3",
        "eslint": "9.39.2",
        "eslint-config-next": "15.5.10",
        "eslint-config-prettier": "10.1.8",
